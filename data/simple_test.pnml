<?xml version="1.0" encoding="UTF-8"?>
<!--
  Simple Petri Net - Diamond Model
  4 places, 2 transitions, 3 reachable states
  Has deadlock at [0,0,0,1]
-->
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="simple_net" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <page id="page1">
      <place id="p0"><initialMarking><text>1</text></initialMarking></place>
      <place id="p1"></place>
      <place id="p2"></place>
      <place id="p3"></place>
      
      <transition id="t0"></transition>
      <transition id="t1"></transition>
      
      <arc id="a1" source="p0" target="t0"/>
      <arc id="a2" source="t0" target="p1"/>
      <arc id="a3" source="t0" target="p2"/>
      <arc id="a4" source="p1" target="t1"/>
      <arc id="a5" source="p2" target="t1"/>
      <arc id="a6" source="t1" target="p3"/>
    </page>
  </net>
</pnml>
